{
  "description": "智能状态自动机",
  "manifest_version": 2,
  "name": "afsm",
  "version": "1.0.0",
  "author": "dexter",
  "icons": {
    "100": "logo.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs", "<all_urls>"],
  "devtools_page": "devtools-page.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"],
      "run_at": "document_start"
    }
  ]
}
